                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:fontFamily="sans-serif-condensed"
                                            android:text="23/09/24 - 19:00"
                                            android:textAppearance="?attr/textAppearanceTitleMedium" />
